{
  "name": "@mongosh/service-provider-core",
  "version": "5.0.1",
  "description": "MongoDB Shell Core Service Provider Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "config": {
    "unsafe-perm": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "prepublish": "pnpm run compile",
    "test": "mocha",
    "test-ci": "node ../../scripts/run-if-package-requested.js pnpm test",
    "test-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test",
    "test-ci-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test-ci",
    "eslint": "eslint",
    "lint": "pnpm run eslint . && pnpm run prettier -- --check .",
    "check": "pnpm run lint",
    "prettier": "prettier",
    "reformat": "pnpm run prettier -- --write . && pnpm run eslint -- --fix"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "dependencies": {
    "@mongosh/errors": "2.4.6",
    "@mongosh/shell-bson": "3.0.1",
    "bson": "^7.2.0",
    "mongodb": "^7.1.0",
    "mongodb-build-info": "^1.9.5",
    "mongodb-connection-string-url": "^7.0.1"
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-mongosh": "^1.0.0",
    "@mongodb-js/prettier-config-devtools": "^1.0.1",
    "@mongodb-js/tsconfig-mongosh": "^1.0.0",
    "eslint": "^7.25.0",
    "prettier": "^2.8.8"
  }
}
