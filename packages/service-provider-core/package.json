{
  "name": "@mongosh/service-provider-core",
  "version": "3.6.3",
  "description": "MongoDB Shell Core Service Provider Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "config": {
    "unsafe-perm": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mongodb-js/mongosh.git"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "prepublish": "npm run compile",
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register --reporter \"../../configs/mocha-config-mongosh/reporter.ts\" \"./src/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "test-coverage": "nyc --no-clean --cwd ../.. --reporter=none npm run test",
    "test-ci-coverage": "nyc --no-clean --cwd ../.. --reporter=none npm run test-ci",
    "eslint": "eslint",
    "lint": "npm run eslint . && npm run prettier -- --check .",
    "check": "npm run lint && npm run depcheck",
    "depcheck": "depcheck",
    "prettier": "prettier",
    "reformat": "npm run prettier -- --write . && npm run eslint -- --fix"
  },
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "dependencies": {
    "@mongosh/errors": "2.4.4",
    "@mongosh/shell-bson": "1.0.3",
    "bson": "^7.0.0",
    "mongodb": "^7.0.0",
    "mongodb-build-info": "^1.8.1",
    "mongodb-connection-string-url": "^7.0.0"
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-mongosh": "^1.0.0",
    "@mongodb-js/prettier-config-devtools": "^1.0.1",
    "@mongodb-js/tsconfig-mongosh": "^1.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^7.25.0",
    "prettier": "^2.8.8"
  }
}
