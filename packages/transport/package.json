{
  "name": "mongodbsh-transport",
  "version": "0.0.0",
  "description": "MongoDB Shell Transport Package",
  "main": "dist/index.js",
  "scripts": {
    "compile": "cross-env NODE_ENV=production webpack --config ./config/webpack.prod.config.js --progress",
    "pretest": "cross-env NODE_ENV=test webpack --config ./config/webpack.test.config.js --progress",
    "test": "mocha --colors \"./src/**/*.spec.js\"",
    "check": "mongodb-js-precommit",
    "prepublish": "npm run compile"
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ],
    "ignore": [
      "sinon", "chai"
    ]
  },
  "peerDependencies": {
    "mongodb": "^3.3.3"
  }
}
