{
  "name": "@mongosh/autocomplete",
  "version": "5.1.0",
  "description": "MongoDB Shell Autocomplete Package",
  "homepage": "https://github.com/mongodb-js/mongosh",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mongosh"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "scripts": {
    "test": "mocha",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "test-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test",
    "test-ci-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test-ci",
    "eslint": "eslint",
    "lint": "pnpm run eslint . && pnpm run prettier -- --check .",
    "check": "pnpm run lint",
    "compile": "tsc -p tsconfig.json",
    "prepublish": "pnpm run compile",
    "prettier": "prettier",
    "reformat": "pnpm run prettier -- --write . && pnpm run eslint -- --fix"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-mongosh": "^1.0.0",
    "@mongodb-js/prettier-config-devtools": "^1.0.1",
    "@mongodb-js/tsconfig-mongosh": "^1.0.0",
    "eslint": "^7.25.0",
    "mocha": "^11.7.5",
    "prettier": "^2.8.8"
  },
  "dependencies": {
    "@mongodb-js/mongodb-constants": "^0.20.1",
    "@mongosh/shell-api": "^5.1.0",
    "@mongodb-js/mongodb-ts-autocomplete": "^0.6.5",
    "semver": "^7.5.4"
  }
}
