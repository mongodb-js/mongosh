{
  "name": "@mongosh/autocomplete",
  "version": "0.0.0-dev.0",
  "description": "MongoDB Shell Autocomplete Package",
  "homepage": "https://github.com/mongodb-js/mongosh",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mongosh"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "scripts": {
    "test": "mocha -r \"../../scripts/import-expansions.js\" --timeout 60000 -r ts-node/register \"./{src,lib}/**/*.spec.ts\"",
    "test-ci": "node ../../scripts/run-if-package-requested.js npm test",
    "eslint": "eslint --report-unused-disable-directives \"./{src,test}/**/*.{js,ts,tsx}\"",
    "lint": "npm run eslint",
    "check": "npm run lint && npm run depcheck",
    "depcheck": "depcheck",
    "compile": "tsc -p tsconfig.json",
    "prepublish": "npm run compile"
  },
  "mongosh": {
    "unitTestsOnly": true
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "depcheck": "^1.4.3",
    "mocha": "^10.2.0"
  },
  "dependencies": {
    "@mongodb-js/mongodb-constants": "^0.2.2",
    "@mongosh/shell-api": "0.0.0-dev.0",
    "semver": "^7.3.2"
  }
}
