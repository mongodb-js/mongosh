{
  "name": "@mongosh/logging",
  "version": "5.0.1",
  "description": "MongoDB Shell Logging Utilities Package",
  "homepage": "https://github.com/mongodb-js/mongosh",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mongosh"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "dependencies": {
    "@mongodb-js/device-id": "^0.4.6",
    "@mongodb-js/devtools-connect": "^3.14.9",
    "@mongosh/types": "^5.0.1",
    "mongodb-log-writer": "^2.5.6",
    "mongodb-redact": "^1.4.6",
    "native-machine-id": "^0.3.6"
  },
  "devDependencies": {
    "@mongodb-js/eslint-config-mongosh": "^1.0.0",
    "@mongodb-js/prettier-config-devtools": "^1.0.1",
    "@mongodb-js/tsconfig-mongosh": "^1.0.0",
    "@segment/analytics-node": "^1.3.0",
    "eslint": "^7.25.0",
    "prettier": "^2.8.8",
    "sinon": "^19.0.4",
    "@mongosh/errors": "2.4.6"
  },
  "scripts": {
    "test": "mocha",
    "test-ci": "node ../../scripts/run-if-package-requested.js pnpm test",
    "test-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test",
    "test-ci-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test-ci",
    "eslint": "eslint",
    "lint": "pnpm run eslint . && pnpm run prettier -- --check .",
    "check": "pnpm run lint",
    "compile": "tsc -p tsconfig.json",
    "prepublish": "pnpm run compile",
    "prettier": "prettier",
    "reformat": "pnpm run prettier -- --write . && pnpm run eslint -- --fix"
  },
  "mongosh": {
    "unitTestsOnly": true
  }
}
