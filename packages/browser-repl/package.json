{
  "name": "@mongosh/browser-repl",
  "version": "0.0.1-alpha.1",
  "description": "Browser presentation component for Mongo Shell",
  "engines": {
    "node": "^12.4.0"
  },
  "main": "lib/mongosh-browser-repl.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.config.prod.js",
    "build:stats": "webpack --config ./config/webpack.config.stats.js",
    "prepublish": "rm -rf ./lib && npm run build",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test": "karma start ./config/karma.conf.js --colors",
    "test-ci": "npm run lint && npm run test",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "check": "npm run lint",
    "compile-ts": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/generator": "^7.8.3",
    "@babel/parser": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@babel/template": "^7.8.3",
    "@leafygreen-ui/icon": "^4.0.0",
    "@leafygreen-ui/palette": "^2.0.0",
    "@leafygreen-ui/syntax": "^2.2.0",
    "@mongosh/browser-runtime-core": "file:../browser-runtime-core",
    "@mongosh/i18n": "file:../i18n",
    "@mongosh/service-provider-core": "file:../service-provider-core"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.0",
    "@storybook/addon-knobs": "^5.3.10",
    "@storybook/react": "^5.3.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "babel-loader": "^8.0.6",
    "bson": "^4.0.3",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^5.1.0",
    "karma-webpack": "^4.0.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mongodb-ace-theme": "^0.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-ace": "^6.6.0",
    "react-dom": "^16.12.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "peerDependencies": {
    "mongodb-ace-theme": "^0.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-ace": "^6.6.0",
    "react-dom": "^16.12.0"
  }
}
