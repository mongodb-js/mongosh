{
  "env": {
    "mocha": true,
    "node": true
  },
  "globals": {
    "expect": true,
    "sinon": true,
    "Proxy": true
  },
  "rules": {
    "camelcase": 1,
    "new-cap": 0,
    "no-use-before-define": 0
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": [
    "mongodb-js/node",
    "mongodb-js/browser"
  ]
}
