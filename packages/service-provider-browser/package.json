{
  "name": "@mongosh/service-provider-browser",
  "version": "0.0.1-alpha.0",
  "description": "MongoDB Shell Service Provider Browser Package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "compile-ts": "tsc -p tsconfig.json",
    "test": "karma start --colors",
    "test-ci": "karma start --colors",
    "prepublish": "npm run compile-ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^12.4.0"
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ],
    "ignore": [
      "sinon",
      "chai",
      "uuid",
      "encoding"
    ]
  },
  "dependencies": {
    "@mongosh/i18n": "file:../i18n",
    "@mongosh/service-provider-core": "file:../service-provider-core",
    "mongodb-stitch-browser-sdk": "^4.7.0"
  }
}
