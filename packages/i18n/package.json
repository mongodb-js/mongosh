    "test-ci": "mocha -r ts-node/register \"./src/**/*.spec.ts\""
  },
  "dependency-check": {
    "entries": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "i18n": "^0.8.4"
  }
}
