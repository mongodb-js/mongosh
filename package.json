{
  "name": "mongosh",
  "version": "0.0.0",
  "description": "The MongoDB Shell",
  "main": "index.js",
  "bin": "packages/cli-repl/bin/mongosh.js",
  "scripts": {
    "prebootstrap": "npm install",
    "bootstrap": "lerna exec -- npm install --no-package-lock",
    "clean": "lerna clean -y && rm -Rf node_modules",
    "check": "lerna run check --since HEAD --exclude-dependents",
    "test": "lerna run test",
    "test-ci": "lerna run test-ci",
    "compile-ts": "lerna run compile-ts",
    "compile-api": "lerna run compile-api",
    "start-antlr": "lerna exec npm run start-antlr --scope mongosh-cli-repl",
    "compile-cli": "lerna run compile-ts --scope mongosh-cli-repl --include-dependencies",
    "prestart-cli": "npm run compile-cli",
    "start-cli": "lerna exec npm start --scope mongosh-cli-repl",
    "compile-browser": "lerna run build --scope mongosh-browser-repl",
    "start-browser": "lerna exec npm start --scope mongosh-browser-repl",
    "compile-compass": "npm run compile-browser && lerna run compile --scope @mongodb-js/compass-shell",
    "start-compass": "lerna run start-compass --scope @mongodb-js/compass-shell",
    "start": "npm run start-cli",
    "precompile-exec": "npm run compile-cli",
    "compile-exec": "pkg packages/cli-repl/bin/mongosh.js --out-path dist"
  },
  "keywords": [
    "MongoDB",
    "Shell",
    "Node",
    "JS",
    "Compass",
    "Stitch"
  ],
  "license": "SSPL",
  "engines": {
    "node": "^12.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "browserify": "latest",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-mongodb-js": "^5.0.3",
    "eslint-plugin-mocha": "^6.2.2",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^4.1.1",
    "lerna": "^3.10.7",
    "mocha": "^6.2.1",
    "mongodb-js-precommit": "^2.0.0",
    "mongodb-runner": "^4.7.5",
    "nexe": "^3.3.2",
    "pkg": "^4.4.3",
    "puppeteer": "^2.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.4.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "mongosh-cli-repl": "file:packages/cli-repl",
    "mongosh-i18n": "file:packages/i18n",
    "mongosh-mapper": "file:packages/mapper",
    "mongosh-service-provider-core": "file:packages/service-provider-core",
    "mongosh-service-provider-server": "file:packages/service-provider-server",
    "mongosh-shell-api": "file:packages/shell-api",
    "mongosh-transport-core": "file:packages/transport-core",
    "mongosh-transport-browser": "file:packages/transport-browser",
    "mongosh-transport-server": "file:packages/transport-server",
    "mongosh-browser-runtime-core": "file:packages/browser-runtime-core",
    "mongosh-browser-runtime-electron": "file:packages/browser-runtime-electron"
  }
}
