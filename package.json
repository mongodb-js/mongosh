{
  "name": "mongosh",
  "version": "0.0.0",
  "description": "The MongoDB Shell",
  "main": "index.js",
  "scripts": {
    "prebootstrap": "npm install",
    "bootstrap": "lerna exec -- npm install --no-package-lock",
    "compile": "lerna run compile",
    "test": "lerna run test",
    "test-ci": "lerna run test-ci",
    "check": "lerna run check",
    "start": "lerna exec npm start --scope mongosh-cli-repl",
    "start-antlr": "lerna exec npm run start-antlr --scope mongosh-cli-repl",
    "prestart": "lerna run compile",
    "build": "lerna exec npm run build --scope=mongosh-shell-api --scope=mongosh-mapper"
  },
  "keywords": [
    "MongoDB",
    "Shell",
    "Node",
    "JS",
    "Compass",
    "Stitch"
  ],
  "license": "SSPL",
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint-config-mongodb-js": "^5.0.3",
    "js-yaml": "^3.13.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^4.1.1",
    "lerna": "^3.10.7",
    "mocha": "^6.2.1",
    "mongodb-js-precommit": "^2.0.0",
    "mongodb-runner": "^4.7.5",
    "puppeteer": "^2.0.0",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "mongosh-cli-repl": "file:packages/cli-repl",
    "mongosh-mapper": "file:packages/mapper",
    "mongosh-service-provider-core": "file:packages/service-provider-core",
    "mongosh-service-provider-server": "file:packages/service-provider-server",
    "mongosh-shell-api": "file:packages/shell-api",
    "mongosh-transport-core": "file:packages/transport-core",
    "mongosh-transport-browser": "file:packages/transport-browser",
    "mongosh-transport-server": "file:packages/transport-server"
  }
}
