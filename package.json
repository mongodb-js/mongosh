{
  "name": "mongosh",
  "version": "0.0.0",
  "description": "The MongoDB Shell",
  "main": "index.js",
  "scripts": {
    "prebootstrap": "npm install",
    "bootstrap": "lerna exec npm install",
    "compile": "lerna run compile",
    "test": "lerna run test",
    "test-ci": "lerna run test-ci",
    "check": "lerna run check",
    "start": "lerna exec npm start --scope mongosh-cli-repl",
    "prestart": "lerna run compile",
    "build": "lerna exec npm run build --scope mongosh-shell-api"
  },
  "keywords": [
    "MongoDB",
    "Shell",
    "Node",
    "JS",
    "Compass",
    "Stitch"
  ],
  "license": "SSPL",
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint-config-mongodb-js": "^5.0.3",
    "js-yaml": "^3.13.1",
    "lerna": "^3.10.7",
    "mocha": "^6.2.1",
    "mongodb-js-precommit": "^2.0.0",
    "mongodb-runner": "^4.7.5",
    "sinon": "^7.5.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "mongosh-cli-repl": "file:packages/cli-repl",
    "mongosh-mapper": "file:packages/mapper",
    "mongosh-service-provider": "file:packages/service-provider",
    "mongosh-shell-api": "file:packages/shell-api",
    "mongosh-transport": "file:packages/transport",
    "mongosh-transport-core": "file:packages/transport-core"
  }
}
