{
  "name": "@mongodb-js/eslint-config-mongosh",
  "version": "1.0.0",
  "private": true,
  "description": "Shared Mongosh eslint configuration",
  "license": "SSPL",
  "main": "index.js",
  "files": [
    "index.js",
    "utils.js",
    "package.json",
    "README.md"
  ],
  "peerDependencies": {
    "eslint": "^7.25.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@mongodb-js/eslint-config-devtools": "^0.9.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  },
  "scripts": {
    "check": "pnpm run lint",
    "lint": "prettier --check .",
    "reformat": "prettier --write .",
    "test": "echo \"no tests\"",
    "test-ci": "pnpm run test",
    "test-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test",
    "test-ci-coverage": "nyc --no-clean --cwd ../.. --reporter=none pnpm run test-ci"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/mongosh.git"
  },
  "homepage": "https://github.com/mongodb-js/mongosh"
}
